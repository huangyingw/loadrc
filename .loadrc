#!/bin/bash
OS=`uname`
. $HOME/loadrc/.androidrc
. $HOME/loadrc/.dockerrc
. $HOME/loadrc/.envrc
. $HOME/loadrc/.gitrc
. $HOME/loadrc/.hgrc
. $HOME/loadrc/.kvmrc
if [ "$OS" == "Darwin" ]
then
    . $HOME/loadrc/."`hostname`".osxrc
fi
. $HOME/loadrc/.p4rc
. $HOME/loadrc/.pythonrc
. $HOME/loadrc/.shellrc
. $HOME/loadrc/.sqlrc
. $HOME/loadrc/.svnrc
. $HOME/loadrc/.zerotierrc
. $HOME/loadrc/openrc.sh

HOSTRC=$HOME/loadrc/."`hostname`".rc
if [ -f "$HOSTRC" ]
then
    . "$HOSTRC"
fi
