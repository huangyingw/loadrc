#!/bin/bash

SLOADRC=$HOME/loadrc/."`hostname`".sloadrc
if [ -f "$SLOADRC" ]
then
    $HOME/loadrc/bashrc/sload.sh
fi

. $HOME/loadrc/.androidrc
. $HOME/loadrc/.dockerrc
. $HOME/loadrc/.envrc
. $HOME/loadrc/.gitrc
. $HOME/loadrc/.hgrc
. $HOME/loadrc/.kvmrc
. $HOME/loadrc/.osxrc
. $HOME/loadrc/.p4rc
. $HOME/loadrc/.shellrc
. $HOME/loadrc/.sqlrc
. $HOME/loadrc/.svnrc
. $HOME/loadrc/.vbunturc
. $HOME/loadrc/openrc.sh

HOSTRC=$HOME/loadrc/."`hostname`".rc
if [ -f "$HOSTRC" ]
then
    . "$HOSTRC"
fi
