[remote "origin"]
    url = ssh://git@bitbucket.org/huangyingw/loadrc.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[remote "gitlab"]
    url = git@gitlab.com:huangyingw/loadrc.git
    fetch = +refs/heads/*:refs/remotes/gitlab/*
[remote "github"]
    url = git@github.com:huangyingw/loadrc.git
    fetch = +refs/heads/*:refs/remotes/github/*
[push]
    remote = origin
    remote = gitlab
    remote = github
[gsync]
    remote = origin
    branch = master
[merge]
    allow = true
[commit]
    message = n
[branch "master"]
    remote = origin
    merge = refs/heads/master
[branch "async"]
    remote = origin
    merge = refs/heads/async
[vrun]
    silent = false
