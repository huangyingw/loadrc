[remote "origin"]
    url = ssh://git@bitbucket.org/huangyingw/loadrc.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[remote "gitlab"]
    url = git@gitlab.com:huangyingw/loadrc.git
    fetch = +refs/heads/*:refs/remotes/gitlab/*
[remote "github"]
    url = git@github.com:huangyingw/loadrc.git
    fetch = +refs/heads/*:refs/remotes/github/*
[push]
    remote = origin
    remote = gitlab
    remote = github
[merge]
    allow = true
[commit]
    message = n
[branch "cscope/cscope_dynamic"]
    remote = origin
    merge = refs/heads/cscope/cscope_dynamic
[branch "cscope"]
    remote = origin
    merge = refs/heads/cscope
[branch "master"]
    remote = origin
    merge = refs/heads/master
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[branch "zsh"]
    remote = origin
    merge = refs/heads/zsh
[branch "remove/nvim"]
    remote = origin
    merge = refs/heads/remove/nvim
[branch "IndexedSearch/dev"]
    remote = origin
    merge = refs/heads/IndexedSearch/dev
[branch "cscope/autoload"]
    remote = origin
    merge = refs/heads/cscope/autoload
[branch "RunShell"]
    remote = origin
    merge = refs/heads/RunShell
[branch "async"]
    remote = origin
    merge = refs/heads/async
[vrun]
    silent = false
    async = false
[deploy]
    host = localhost
    path = ~/loadrc
[gsync]
    remote = origin
    branch = gcob
[gdif]
    ignore = .ssh
    ignore = jdupes
    ignore = oh-my-zsh
    ignore = rsync.files
    ignore = vimrc/.vim/bundle/split-term.vim
    ignore = vimrc/.vim/bundle/vimtex
