new file mode 100755
index 00000000..03ad0cc4
--- /dev/null
+++ ./bashrc/cutcommon.sh
@@ -0,0 +1,7 @@
+#!/bin/zsh
+export LC_ALL=C
+
+comm -2 -3 <(sort "$1") <(sort "$2") > "$1".findresult
+comm -1 -3 <(sort "$1") <(sort "$2") > "$2".findresult 
+cp -fv "$1".findresult "$1"
+cp -fv "$2".findresult "$2"
