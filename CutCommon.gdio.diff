diff --git a/vishrc/cat_rate.sh b/vishrc/cat_rate.sh
new file mode 100755
index 00000000..5ace0203
--- /dev/null
+++ ./vishrc/cat_rate.sh
@@ -0,0 +1,8 @@
+#!/bin/zsh
+
+while read ss
+do
+    source=$(echo "$ss" | sed 's/"//g')
+    target=$(echo "$ss" | sed "s/\/[6-9]\s\([^\/]*$\)/\/\1/g;s/\/\([^\/]*$\)/\/$2 \1/g;s/  \+/ /g;s/\"//g")
+    mv -nv "$source" "$target"
+done < "$1"
